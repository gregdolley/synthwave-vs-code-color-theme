  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregdolley/synthwave-vs-code-color-theme/"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "keywords": [
    "retro",
    "80s"
